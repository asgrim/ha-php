{
    "name": "asgrim/ha-php",
    "description": "HA PHP agent",
    "type": "project",
    "require": {
        "php": ">= 7.3.0, < 7.5.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "laminas/laminas-diactoros": "^2.3",
        "monolog/monolog": "^2.1",
        "php-http/curl-client": "^2.1",
        "php-http/discovery": "^1.8",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "symfony/console": "^5.1",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Asgrim\\HaPhp\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3.14"
        }
    }
}
